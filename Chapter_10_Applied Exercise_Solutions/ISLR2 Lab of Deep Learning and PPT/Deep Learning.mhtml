# Section 4: Build Text Classifier


``` r
# Convert to torch tensors
x_train_t &lt;- torch_tensor(x_train_padded, dtype =3D torch_long())
y_train_t &lt;- torch_tensor(y_train, dtype =3D torch_float32())$view(c(-1,=
 1))
x_test_t &lt;- torch_tensor(x_test_padded, dtype =3D torch_long())
y_test_t &lt;- torch_tensor(y_test, dtype =3D torch_float32())$view(c(-1, 1=
))

# Define text classifier
text_classifier &lt;- nn_module(
  "TextClassifier",
  initialize =3D function(vocab_size =3D 10000, embedding_dim =3D 32, hidde=
n_dim =3D 16) {
    # Embedding layer: converts word indices to dense vectors
    self$embedding &lt;- nn_embedding(vocab_size + 1, embedding_dim, paddin=
g_idx =3D 1)
    self$fc1 &lt;- nn_linear(embedding_dim, hidden_dim)
    self$fc2 &lt;- nn_linear(hidden_dim, 1)
  },
  forward =3D function(x) {
    x %&gt;%
      self$embedding() %&gt;%
      torch_mean(dim =3D 2) %&gt;%  # Global average pooling
      self$fc1() %&gt;%
      nnf_relu() %&gt;%
      self$fc2() %&gt;%
      nnf_sigmoid()
  }
)
```

