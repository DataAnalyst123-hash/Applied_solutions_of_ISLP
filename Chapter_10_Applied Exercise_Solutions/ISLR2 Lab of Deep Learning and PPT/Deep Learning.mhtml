                 dim =3D c(nrow(mnist_data$train$x), 784)) / 255
y_train &lt;- mnist_data$train$y
x_test &lt;- array(mnist_data$test$x,=20
                dim =3D c(nrow(mnist_data$test$x), 784)) / 255
y_test &lt;- mnist_data$test$y
